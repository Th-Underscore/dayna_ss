adata_separator": "\n", "text": "For Paul.", "mimetype": "text/plain", "start_char_idx": null, "end_char_idx": null, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}, "3_6_7": {"__data__": {"id_": "3_6_7", "embedding": null, "metadata": {"message_idx": 3, "paragraph_idx": 6, "sentence_idx": 7, "timestamp": "September 5th, 2027T12:54 PM", "speakers": [], "characters_present": [], "subjects_referenced": {"characters": [], "groups": [], "events": []}, "scene_id": null, "event_id": null, "is_summary": false}, "excluded_embed_metadata_keys": [], "excluded_llm_metadata_keys": [], "relationships": {}, "metadata_template": "{key}: {value}", "metadata_separator": "\n", "text": "I had to.", "mimetype": "text/plain", "start_char_idx": null, "end_char_idx": null, "metadata_seperator": "\n", "text_template": "{metadata_str}\n\n{content}", "class_name": "TextNode"}, "__type__": "1"}}, "docstore/metadata": {"2_summary": {"doc_hash": "39222247d3221eb0e4b4629d5f8c4e1a500c457cfb60edfb0e18d267ea383978"}, "3_summary": {"doc_hash": "2e4e01e576d9826816d21d32ffecbc243b347d6a1f0712dfe40aa5a8d202e0c9"}, "3_1_1": {"doc_hash": "74dfcf059a8c5b5f70a3581948f8f71242c02e06f566cd0029256ad06d435d17"}, "3_1_2": {"doc_hash": "1e8ee37d49378018392d17377aee05211e9850e43dfe28fd2a300246b12030f2"}, "3_1_3": {"doc_hash": "e455695f34c75f152dd933a195f1415cd6efb42987656976a09aaa65b6661891"}, "3_1_4": {"doc_hash": "eeaf8d3c350cc800e10a6a454b28c71946afb634732ae5505904f3682e1f0027"}, "3_1_5": {"doc_hash": "e03c01a6679e56a8840bd7953e274f85fccb76e12d873a0a156effbc63ffd7f9"}, "3_1_6": {"doc_hash": "bd065f2bc27ea85041ef538e353b96fb355a3b62ee7ce22eecc523987369b2af"}, "3_1_7": {"doc_hash": "22950ffc5010f69c209bf6636ea02e6f3d6a9cb642b0d54654ac68ae510e7939"}, "3_1_8": {"doc_hash": "e01bc18587db8d14368563d476cad7bceef1c6918f91746d37421d1e670fe3b8"}, "3_2_1": {"doc_hash": "51810c307bed7ff089ff5a0e5fe57a4e3b7d7c71a01cf4fde67b15806d346a69"}, "3_2_2": {"doc